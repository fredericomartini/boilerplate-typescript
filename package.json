{
  "name": "action-reaction",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@sentry/node": "^5.27.0",
    "@types/jest": "^26.0.15",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "elastic-apm-node": "^3.7.0",
    "express": "^4.17.1",
    "fluent-logger": "^3.4.1",
    "helmet": "^4.2.0",
    "ioredis": "^4.17.3",
    "jest": "^26.6.0",
    "jest-extended": "^0.11.5",
    "module-alias": "^2.2.2",
    "rate-limiter-flexible": "^2.1.10",
    "reflect-metadata": "^0.1.13",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "typedi": "^0.8.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.17.6",
    "@types/js-yaml": "^3.12.5",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.0",
    "@types/sinon": "^9.0.8",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "handlebars": "^4.7.6",
    "js-yaml": "^3.14.0",
    "json-schema-ref-parser": "^9.0.6",
    "node-mocks-http": "^1.9.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "start": "pm2 start process.yml",
    "stop": "pm2 stop process.yml",
    "dev": "nodemon src/index.ts",
    "debug": "nodemon --inspect=0.0.0.0:9229 src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "npm run test:unity && npm run test:functional",
    "test:unity": "env-cmd jest --logHeapUsage UNITY",
    "test:functional": "env-cmd jest --runInBand --logHeapUsage FUNCTIONAL",
    "test:clear": "env-cmd jest --clearCache",
    "test:watch": "npm run test:clear && env-cmd jest --runInBand --logHeapUsage --watchAll",
    "test:coverage": "rimraf coverage && npm run test:clear && env-cmd jest  --runInBand --all --logHeapUsage --coverage ",
    "build": "rimraf dist && tsc -b",
    "generate-docs": "env-cmd ts-node src/config/generateDocs.ts"
  }
}
